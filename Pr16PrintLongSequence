using System;

class Pr16PrintLongSequence
{
    static void Main()
    {
        int num = 2;
        Console.WriteLine(num);
        for (int i = 0; i < 1000; i++)
        {
            num = num + 1;
            if (i % 2 == 0)
            {
                Console.WriteLine("-" + num);
            }
            else
            {
                Console.WriteLine(num);
            }
        }
    }
