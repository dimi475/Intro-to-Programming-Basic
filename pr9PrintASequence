using System;

class PrintASequence
{
    static void Main()
    {
        int num = 2;
        Console.WriteLine(num);
        for (int i = 0; i < 9; i++)
        {
            num = num + 1;
            if (i % 2 == 0)
            {
                Console.WriteLine("-"+num);
            }
            else
            {
                Console.WriteLine(num);
            }
        }
    }
}
